{
  "name": "nodegui-plugin",
  "version": "1.0.0",
  "description": "A sample native plugin example for NodeGUI",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:addon",
    "build:addon": "node-gyp -j 8 configure build",
    "rebuild:addon": "node-gyp -j 8 rebuild",
    "dev": "npm run build && qode dist/demo.js"
  },
  "author": "Atul R",
  "license": "MIT",
  "peerDependencies": {
    "@nodegui/nodegui": "*"
  },
  "dependencies": {
    "node-addon-api": "^1.7.1"
  },
  "devDependencies": {
    "@nodegui/nodegui": "^0.1.9",
    "@nodegui/qode": "^1.0.5",
    "@types/node": "^12.7.5",
    "typescript": "^3.6.3"
  }
}
